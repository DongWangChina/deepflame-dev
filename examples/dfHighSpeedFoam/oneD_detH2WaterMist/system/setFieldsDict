FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// defaultFieldValues
// (
//     volVectorFieldValue U (0 0 0)
//     volScalarFieldValue T 300
//     volScalarFieldValue p 20265
// );

regions
(
    boxToCell
    {
        box (0.6 0 0) (1.4 0.01 0.01);
        fieldValues
        (
            volScalarFieldValue T 300
            volScalarFieldValue p 20265 
            volVectorFieldValue U (0 0 0)
            volScalarFieldValue AR 0.88587
            volScalarFieldValue H2 0.01277
            volScalarFieldValue O2 0.10136
            volScalarFieldValue CO 0.0
            volScalarFieldValue CO2 0.0
            volScalarFieldValue H2O 0.0
            volScalarFieldValue H 0.0
            volScalarFieldValue H2O2 0.0
            volScalarFieldValue HE 0.0
            volScalarFieldValue HO2 0.0
            volScalarFieldValue N2 0.0
            volScalarFieldValue O 0.0
            volScalarFieldValue OH 0.0
        );
    }
);


